
{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true

  },
  "rules": {
    "import/no-unresolved": [
      2,
      { "caseSensitive":  false}
    ],
    "array-callback-return": 0
  }
}




