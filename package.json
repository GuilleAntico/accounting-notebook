{
  "name": "codesample",
  "version": "1.0.0",
  "description": "Food Truck Example with nodejs and react",
  "main": "server.js",
  "scripts": {
    "test": "env LOCAL_TESTING=TRUE NODE_ENV=test npm run unit:test && npm run integration:test",
    "unit:test": "env LOCAL_TESTING=TRUE NODE_ENV=test mocha tests/bootstrap.js tests/unit/* --recursive -t 30000 --exit",
    "integration:test": "env LOCAL_TESTING=TRUE NODE_ENV=test mocha tests/bootstrap.js tests/integration/* --recursive -t 30000 --exit",
    "start": "node ./server.js && npm run start:workers",
    "start:dev": "./node_modules/.bin/nodemon ./server.js",
    "seed:db": "./node_modules/.bin/knex seed:run --knexfile app/config/knexfile.js",
    "migrate:latest": "./node_modules/.bin/knex migrate:latest --knexfile app/config/knexfile.js",
    "migrate:rollback": "./node_modules/.bin/knex migrate:rollback --knexfile app/config/knexfile.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-specific": "eslint $PATH"
  },
  "author": "Carlos Guillermo Antico",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.14.2",
    "chalk": "^2.4.1",
    "config": "^3.0.1",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.2",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "node-uuid": "^1.4.8",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "rootpath": "^0.1.2",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0"
  },
  "_moduleAliases": {
    "app": "./app",
    "communications": "./communications",
    "devUtilities": "./developerUtilities",
    "lib": "./lib",
    "services": "./services",
    "@devConsole": "./developerUtilities/devConsole",
    "@authenticate": "app/middlewares/authenticate"
  }
}
